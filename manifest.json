{
  "name": "AWS Logs",
  "version": "1.0.0",
  "description": "Adds a link to the corresponding cloudwatch log group for an ECS app or Lambda function.",
  "content_scripts": [
    {
      "matches": ["https://console.aws.amazon.com/ecs/*"],
      "run_at": "document_end",
      "js": ["./scripts/betterLogs.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "images/cloudwatch_icon.png",
    "32": "images/cloudwatch_icon.png",
    "48": "images/cloudwatch_icon.png",
    "128": "images/cloudwatch_icon.png"
  },
  "manifest_version": 2
}